<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cxl.carpro.dao.mapper.CarInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cxl.carpro.dao.model.CarInfoDO">
        <id column="id" property="id" />
        <result column="car_model" property="carModel" />
        <result column="car_stock" property="carStock" />
        <result column="total" property="total" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, car_model, car_stock, total
    </sql>

</mapper>
